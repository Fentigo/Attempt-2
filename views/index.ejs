<!DOCTYPE html>
<html lang="en">
<head>
<%- include('../Partials/header.ejs') %>
</head>
<body>
    <%- include('../Partials/nav.ejs') %>
   
    <div class="Blog Content">
        <h2> All Blogs</h2>
        
        <% if(blogs.length > 0){%>
            <% blogs.forEach(blog => { %>
                <h3 class="title"><%= blog.title %></h3>
                <p class="snippet"><%= blog.snippet %></p>
            <% }) %>
        <% }%>


    </div>
    <%- include('../Partials/footer.ejs') %>
</body>


</html>